{
  "name": "@chess-tent/server",
  "version": "1.0.0",
  "description": "Chess tent server",
  "main": "dist/index.js",
  "scripts": {
    "preinstall": "node ./scripts/preinstall.js",
    "start": "nodemon",
    "build:dev": "echo no build",
    "build": "yarn run clean && yarn run compile",
    "clean": "rimraf -rf ./dist",
    "compile": "tsc -p tsconfig.json",
    "test": "yarn test-db-up && yarn jest; yarn test-db-down",
    "test-db-up": "docker-compose up -d",
    "test-db-down": "docker-compose down --remove-orphans",
    "migrate:create": "cd ./src/application/migrations/ && migrate create --template-file ./migrations/_template.ts",
    "migrate:up": "echo 'Run build before migrating'; node ./dist/migration up",
    "migrate:down": "echo 'Run build before migrating'; node ./dist/migration down"
  },
  "author": "Luka Bracanovic",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mailgun-js": "^0.22.11",
    "@types/mongodb": "^3.5.17",
    "@types/mongoose": "^5.10.5",
    "@types/supertest": "^2.0.12",
    "@types/ws": "^7.2.6",
    "migrate": "^2.0.0",
    "supertest": "^6.3.3"
  },
  "dependencies": {
    "@chess-tent/models": "1.0.0",
    "@chess-tent/redux-record": "1.0.0",
    "@chess-tent/types": "1.0.0",
    "@nodepit/migrate-state-store-mongodb": "^3.1.0-beta.0",
    "aws-sdk": "^2.742.0",
    "axios": "^1.2.2",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "core-module": "2.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mailgun-js": "^0.22.0",
    "module-alias": "^2.2.2",
    "mongodb": "3.7.3",
    "mongoose": "5.13.15",
    "socket.io": "^4.4.1",
    "uuid": "^3.4.0",
    "ws": "^7.3.0",
    "immer": "^10.0.2"
  }
}
